<h1>Edit Track</h1>

<%= form_for @track do |f| %>
  <%= f.label "name", "Name:" %>
  <%= f.text_field("name") %>
  <%= f.label "artist", "Artist:" %>
  <%= f.text_field("artist") %>
  <%= f.label "url", "URL:" %>
  <%= f.text_field("url") %>
  <%= f.submit %>
<% end %>

